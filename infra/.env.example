# Tenant identifier (e.g. tenant1)
TENANT=tenant1

# Public URL for this tenant (e.g. tenant1.kestra.seudominio.com)
DOMAIN=tenant1.kestra.seudominio.com

# Postgres
POSTGRES_PASSWORD=change_me_strong_password

# MinIO global (adjust service name if stack name differs)
S3_ENDPOINT=http://infra_minio:9000
S3_BUCKET=kestra-tenant1
S3_REGION=us-east-1
S3_ACCESS_KEY=minio_access_key_for_tenant
S3_SECRET_KEY=minio_secret_key_for_tenant
